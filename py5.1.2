def search_element(array, element):
    for i in range(len(array)):
        if array[i] == element:
            return i
    return -1
if __name__ == "__main__":
    array = [int(x) for x in input("Enter the elements of the array separated by space: ").split()]
    search_element = int(input("Enter the element to search for: "))
    position = search_element(array, search_element)
    if position != -1:
        print(f"The element {search_element} is found at position {position}.")
    else:
        print("The element is not found in the array.")
