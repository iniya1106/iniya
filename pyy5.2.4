array1 = [5, 3, 8, 1, 2, 7, 4, 6]
array2 = [5, 3, 8, 1, 2, 7, 4, 6]
are_same = True
if len(array1) != len(array2):
    are_same = False
else:
    for i in range(len(array1)):
        if array1[i] != array2[i]:
            are_same = False
            break

if are_same:
    print("The arrays are the same.")
else:
    print("The arrays are not the same.")
